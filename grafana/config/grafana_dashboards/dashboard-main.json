          "hide": false,
          "query": "import \"strings\"\r\n\r\nfirstTestName = \"EX1\"\r\n\r\ngetStartTime = (data, testName) => {\r\n  return data\r\n    |> filter(fn: (r) => r.testName == testName)\r\n    |> filter(fn: (r) => r.metric == \"read\" or r.metric == \"update\")\r\n    |> first()\r\n    |> findRecord(fn: (key) => true, idx: 0)\r\n}\r\n\r\ntimeDiff = (t1, t2) => {\r\n  return if exists t2._time then uint(v: t2._time) - uint(v: t1._time) else uint(v: 0)\r\n}\r\n\r\ngetTestData = (data, testName) => {\r\n  t1Start = getStartTime(data: data, testName: firstTestName)\r\n  t2Start = getStartTime(data: data, testName: testName)\r\n  diff = timeDiff(t1: t1Start, t2: t2Start)\r\n  return data\r\n    |> filter(fn: (r) => r.testName == testName)\r\n    |> map(fn: (r) => ({r with _time: time(v: uint(v: r._time) - diff)}))\r\n}\r\n\r\nallData = from(bucket: \"client_metrics_raw\")\r\n  |> range(start: -10y)\r\n  |> map(fn: (r) => ({\r\n    r with \r\n    testGroup: strings.split(v: r.filename, t: \".\")[0],\r\n    testName: strings.split(v: r.filename, t: \".\")[1]\r\n  }))\r\n  |> filter(fn: (r) => r.testGroup == \"${workload}\")\r\n\r\nt1Data = getTestData(data: allData, testName: \"EX1\")\r\nt2Data = getTestData(data: allData, testName: \"EX2\")\r\n\r\nunion(tables: [t1Data, t2Data])\r\n  |> filter(fn: (r) => r.metric != \"cleanup\")\r\n  |> rename(columns: {filename: \"testId\"})\r\n  |> map(fn: (r) => ({ r with testId: (r.testGroup + \".\" + r.testName) }))\r\n",
          "refId": "A"
        }
      ],
      "title": "New Panel",
      "transformations": [
        {
          "id": "renameByRegex",
          "options": {
            "regex": ".*metric=\"([^\"]+)\", testId=\"([^\"]+)\".*",
            "renamePattern": "$2 - $1"
          }
        }
      ],
